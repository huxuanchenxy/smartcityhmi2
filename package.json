{
    "name": "datav-vue",
    "version": "0.0.1",
    "description": "A Powerful Data Visualization Tool",
    "scripts": {
        "gc": "bash ./scripts/gc.sh",
        "new": "plop --plopfile ./plop-templates/plopfile.js",
        "bootstrap": "yarn --frozen-lockfile",
        "dev": "cross-env NODE_ENV=development vite",
        "dev2": "cross-env NODE_ENV=development vue-tsc --noEmit && vite build --mode development",
        "staging": "cross-env NODE_ENV=staging vue-tsc --noEmit && vite build --mode staging",
        "build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build",
        "build:fast": "cross-env NODE_ENV=production vite build",
        "serve": "vite preview",
        "lint": "yarn lint:type && yarn lint:code && yarn lint:css",
        "lint:type": "vue-tsc --noEmit",
        "lint:code": "eslint --fix 'src/**/*.{js,ts,vue}'",
        "lint:css": "stylelint --fix 'src/**/*.(css|scss|vue)'",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "prepare": "husky install",
        "fix-memory-limit": "cross-env LIMIT=2000000000000 increase-memory-limit"
    },
    "dependencies": {
        "@amap/amap-jsapi-loader": "^1.0.1",
        "@kjgl77/datav-vue3": "^1.2.1",
        "@monaco-editor/loader": "^1.1.1",
        "@turf/turf": "^6.5.0",
        "@types/file-saver": "^2.0.5",
        "@types/three": "^0.165.0",
        "@vitejs/plugin-legacy": "^1.8.1",
        "accounting": "^0.4.1",
        "axios": "^0.21.1",
        "axios-mock-adapter": "^1.19.0",
        "crypto-js": "^4.0.0",
        "dayjs": "^1.10.6",
        "echarts": "^5.2.0",
        "echarts-wordcloud": "^2.0.0",
        "element-plus": "1.0.2-beta.55",
        "file-saver": "^2.0.5",
        "gsap": "^3.7.1",
        "html2canvas": "^1.3.2",
        "js-cookie": "^3.0.1",
        "jsonpath-plus": "^8.1.0",
        "lodash-es": "^4.17.21",
        "mockjs": "^1.1.0",
        "monaco-editor": "0.27.0",
        "naive-ui": "^2.30.8",
        "number-precision": "^1.5.1",
        "particles.vue3": "^1.18.1",
        "proj4": "^2.8.0",
        "shortid": "^2.2.16",
        "three": "^0.165.0",
        "typescript-cookie": "^1.0.3",
        "vue": "^3.2.20",
        "vue-baidu-map-3x": "^1.0.35",
        "vue-echarts": "^6.0.0",
        "vue-i18n": "^9.1.9",
        "vue-router": "^4.0.12",
        "vue3-draggable-resizable": "^1.6.1",
        "vue3-openlayers": "^0.1.55",
        "vuex": "4.0.2",
        "vuex-module-decorators": "^1.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@types/accounting": "^0.4.2",
        "@types/crypto-js": "^4.0.1",
        "@types/lodash-es": "^4.17.4",
        "@types/mockjs": "^1.0.3",
        "@types/shortid": "^0.0.29",
        "@typescript-eslint/eslint-plugin": "^4.31.2",
        "@typescript-eslint/parser": "^4.31.2",
        "@vitejs/plugin-vue": "^1.9.3",
        "@vue/compiler-sfc": "^3.2.20",
        "@vue/component-compiler-utils": "^3.2.2",
        "conventional-changelog-cli": "^2.1.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-vue": "^7.18.0",
        "handlebars": "^4.7.7",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "plop": "^2.7.4",
        "rimraf": "^3.0.2",
        "sass": "^1.42.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^22.0.0",
        "typescript": "^4.4.3",
        "vite": "^2.6.7",
        "vite-plugin-monaco-editor": "^1.0.10",
        "vite-plugin-plain-text": "^1.1.4",
        "vue-tsc": "^0.28.6"
    },
    "lint-staged": {
        "src/**/*.{js,ts,vue}": [
            "eslint --fix"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:pengxiaotian/datav-vue.git"
    },
    "homepage": "https://datav.pengxiaotian.com",
    "author": "Xiaotian (Allen) Peng",
    "license": "MIT",
    "browserslist": [
        "> 2%",
        "last 2 versions",
        "not IE 11"
    ]
}